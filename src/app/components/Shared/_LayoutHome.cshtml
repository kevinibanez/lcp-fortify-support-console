
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
  <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
  <meta http-equiv="Pragma" content="no-cache" />
  <meta http-equiv="Expires" content="0" />
  @*<meta http-equiv="Content-Security-Policy" content="default-src * 'unsafe-inline'">*@
  <meta http-equiv="Content-Security-Policy"
        content="
            default-src 'self' *.lcptracker.net;
            connect-src 'self' cdn.contentful.com dc.services.visualstudio.com;
            font-src *;
            style-src 'self' 'unsafe-inline' *;
            script-src 'self' 'unsafe-inline' az416426.vo.msecnd.net;
            img-src 'self' *.lcptracker.net http://cdn.lcptracker.net data: https:;
            object-src 'none';
            base-uri 'none';
          " />
  <title>@ViewData["Title"] - Fortify by LCPtracker, Inc.</title>
  <link rel="icon" type="image/x-icon" href="~/favicon.ico" />
  <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />
  <link href="~/css/bootstrap.css" rel="stylesheet" />
  <link href="~/css/mdbnew.css" rel="stylesheet" />
  <link href="~/css/material.indigo-blue.min.css" rel="stylesheet" />
  <link href="~/css/fontawesome.css" rel="stylesheet" />
  <link href="~/css/fa-brands.css" rel="stylesheet" />
  <link href="~/css/fa-regular.css" rel="stylesheet" />
  <link href="~/css/fa-solid.css" rel="stylesheet" />
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <link href="~/css/main.css" rel="stylesheet">
  <link rel="stylesheet" href="~/css/site.css" />
  <link rel="stylesheet" href="~/css/hsimp.css" />
  <link rel="stylesheet" href="~/css/profileNavComb.css" />
  @RenderSection("css", required: false)

</head>
<body class="dynamic-color">

  <!-- KEYBOARD SHORCUT AND TABS -->
  <span class="skipnav">
    <a id="skip2Content" class="pointer" tabindex="0">
      Skip to main content
    </a>
    <a id="skip2Nav" class="pointer" tabindex="0">
      Skip to navigation
    </a>
  </span>

  <header>
    <!--
    Mobile/DESKTOP Version 1
    -->
    <div class="mobile-container">
      <div class="top-bar"></div>
      <div class="logo">
        <a id="Home-Page" tabindex="0" href="/Home/Index"><img src="https://cdn.lcptracker.net/Common/images/logo.png" alt="LCPtracker Logo" /></a>
      </div>
      <div id="nav-trigger" onclick="toggleHam()">
        <div class="icon">
          <div class="bar b1"><span></span></div>
          <div class="bar b2"><span></span></div>
          <div class="bar b3"><span></span></div>
        </div>
      </div>
      <a class="nav-title" tabindex="0">Menu</a>
    </div>
  </header>
  @await Html.PartialAsync("_MobileMenu")
  <div class="margin-15"></div>


  <main>
    <div class="container">
      <div class="body-content">
        @RenderBody()
      </div>
    </div>
  </main>



 
  <!--
      Discard Changes Global Dialog
  -->
  <div class="modal fade" id="modalWindow" tabindex="-1" role="dialog" aria-labelledby="modalWindowLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header border-0">
          <h5 class="modal-title" id="modalWindowLabel"></h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div id="modalBody" class="modal-body h5">
          This will discard any changes you've made to your profile. Do you want to continue?
        </div>
        <div class="modal-footer mang_act_dtls border-0">
          <button type="button" class="btn_cncl" data-dismiss="modal">Stay on this page</button>
          <button type="button" class="btn_sv" id="Leave">Leave this page</button>
        </div>
      </div>
    </div>
  </div>



  @await Html.PartialAsync("_CookieConsentPartial")
  @await Html.PartialAsync("_NoScriptNotification")

  <!-- JS FRAMEWORKS -->
  <script src="~/js/contentful.browser.min.js"></script>
  <script src="~/js/popper.min.js"></script>
  <script src="~/js/jquery-3.3.1.min.js"></script>
  <script src="~/js/bootstrap.min.js"></script>
  <script src="~/js/material.min.js"></script>
  <script src="/src/assets/js/support_console.js">
  </script>
  @RenderSection("scripts", required: false)
</body>
</html>
